{
  "name": "Hey Loon",
  "version": "2.1",
  "homepage_url": "http://nlaz.github.io",
  "description": "Replaces all images on a given page with pictures of Loons!",
  "icons": { "16"  : "img/icon_16.png",
             "48"  : "img/icon_48.png",
             "128" : "img/icon_128.png"},
  "browser_action": {
    "default_icon": "img/icon_48.png",
    "default_title": "HEY LOON"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js", "heyloon.js"]
  },
  "manifest_version": 2
}
